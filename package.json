{
  "name": "template-server",
  "version": "1.0.0",
  "description": "Sequelize server template for Code Team 42",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-team-42/template-server.git"
  },
  "keywords": [
    "sequelize",
    "MySQL",
    "server",
    "template",
    "express",
    "node"
  ],
  "author": "Code Team 42",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-team-42/template-server/issues"
  },
  "homepage": "https://github.com/code-team-42/template-server#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^5.16.0"
  }
}
